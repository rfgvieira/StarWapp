<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/xnormal"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/background">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="@dimen/normal">
            <TextView
                android:id="@+id/tv_nomedetalhe"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/orange"
                android:textSize="@dimen/txt_normal"
                tools:text="ASDSADASD"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>
            <TextView
                android:id="@+id/attr1detalhe"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/orange"
                android:textSize="@dimen/txt_xnormal"
                android:layout_marginTop="@dimen/xlarge"
                tools:text="24hrs"
                app:layout_constraintTop_toBottomOf="@id/tv_nomedetalhe"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/attr2detalhe"/>
            <TextView
                android:id="@+id/attr2detalhe"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/orange"
                android:textSize="@dimen/txt_xnormal"
                tools:text="24hrs"
                app:layout_constraintTop_toTopOf="@id/attr1detalhe"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/attr1detalhe"/>
            <TextView
                android:id="@+id/attr3detalhe"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/orange"
                android:textSize="@dimen/txt_xnormal"
                android:layout_marginTop="@dimen/normal"
                tools:text="24hrs"
                app:layout_constraintTop_toBottomOf="@id/attr1detalhe"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@id/attr4detalhe"/>
            <TextView
                android:id="@+id/attr4detalhe"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/orange"
                android:textSize="@dimen/txt_xnormal"
                tools:text="24hrs"
                app:layout_constraintTop_toTopOf="@id/attr3detalhe"
                app:layout_constraintStart_toEndOf="@id/attr3detalhe"
                app:layout_constraintEnd_toEndOf="parent"/>
            <TextView
                android:id="@+id/attr5detalhe"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/orange"
                android:textSize="@dimen/txt_xnormal"
                android:layout_marginTop="@dimen/normal"
                tools:text="24hrs"
                app:layout_constraintTop_toBottomOf="@id/attr3detalhe"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@id/attr6detalhe"/>
            <TextView
                android:id="@+id/attr6detalhe"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/orange"
                android:textSize="@dimen/txt_xnormal"
                tools:text="24hrs"
                app:layout_constraintTop_toTopOf="@id/attr5detalhe"
                app:layout_constraintStart_toEndOf="@id/attr5detalhe"
                app:layout_constraintEnd_toEndOf="parent"/>


        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>




</androidx.constraintlayout.widget.ConstraintLayout>